<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
	package="com.example.app_nanonino"
	android:versionCode="1"
	android:versionName="1.0" >

	<uses-permission android:name="android.permission.INTERNET" />

	<uses-sdk
		android:minSdkVersion="11"
		android:targetSdkVersion="19" />

	<application
		android:allowBackup="true"
		android:icon="@drawable/logo"
		android:label="@string/app_name"
		android:theme="@style/CustomActionBarTheme" >
		<activity
			android:name="com.example.app_nanonino.LoginActivity"
			android:label="@string/app_name" >
			<intent-filter>
				<action android:name="android.intent.action.MAIN" />

				<category android:name="android.intent.category.LAUNCHER" />
			</intent-filter>
		</activity>
		<activity
			android:name="com.example.app_nanonino.ContactActivity"
			android:label="@string/title_activity_contact" >
		</activity>
		<activity
			android:name="com.example.app_nanonino.ContactListActivity"
			android:label="@string/title_activity_contact_list" >
			<meta-data
				android:name="android.support.UI_OPTIONS"
				android:value="splitActionBarWhenNarrow" />
		</activity>
		<activity
			android:name="com.example.app_nanonino.FragmentDetalleActivity"
			android:label="@string/title_activity_fragmentdetalle" >
		</activity>
		<activity
			android:name="com.example.app_nanonino.DetalleActivity"
			android:label="@string/title_activity_detalle" >
		</activity>
	</application>

</manifest>
